<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx/8"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="Main8.SignupController"
            prefWidth="400" prefHeight="300">

    <children>
        <StackPane AnchorPane.topAnchor="100"
                   AnchorPane.bottomAnchor="100"
                   AnchorPane.leftAnchor="100"
                   AnchorPane.rightAnchor="100">

            <VBox spacing="10" alignment="CENTER">
                <Label text="Register Form"/>
                <TextField promptText="Username"/>
                <PasswordField promptText="Password"/>
                <Button text="Register"/>
            </VBox>

        </StackPane>
    </children>

</AnchorPane>
